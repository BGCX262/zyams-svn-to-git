			</isNotEmpty>
		</dynamic>
	</select>

	<select id="selectPaginationWithIndex"
		resultClass="java.util.HashMap" parameterClass="java.util.HashMap"
		remapResults="true">
		select RowID,name,zi.id as ID,zi.status as BAK6 from zy_corporation LEFT JOIN
		zy_index zi ON zi.corporationid = RowID
		<isNotEmpty prepend="and" property="status">
			zi.status=#status#
		</isNotEmpty>
		<isNotEmpty prepend="and" property="year">
			zi.year=#year#
		</isNotEmpty>
		<isNotEmpty prepend="and" property="month">
			zi.month=#month#
		</isNotEmpty>
		order by zi.status desc limit #start#,#size#
	</select>
	<select id="selectIndexsWithArea" resultMap="ZyIndexResult"
		parameterClass="java.util.HashMap">
		select * from zy_index zi where EXISTS(select zc.RowID from
		zy_corporation zc where zc.RowID=zi.corporationid and zc.areaID
		=#areaId# and zc.IS50='是') 
		<isGreaterThan prepend="and" property="year" compareValue="0">
			<![CDATA[zi.year=#year#]]>
		</isGreaterThan>
		<isGreaterThan prepend="and" property="month" compareValue="0">
			<![CDATA[zi.month=#month#]]>
		</isGreaterThan>
	</select>
	<select id="selectIndexsWithTrade" resultMap="ZyIndexResult"
		parameterClass="java.util.HashMap">
		select * from zy_index zi where  EXISTS(select zc.RowID from zy_corporation zc where zc.RowID=zi.corporationid and zc.tradeID =#tradeId# and zc.IS50='是')
		<isGreaterThan prepend="and" property="month" compareValue="0">
			<![CDATA[zi.month =#month#]]>
		</isGreaterThan>
		<isGreaterThan prepend="and" property="year" compareValue="1960">
			<![CDATA[zi.year=#year#]]>
		</isGreaterThan>
	</select>
	
