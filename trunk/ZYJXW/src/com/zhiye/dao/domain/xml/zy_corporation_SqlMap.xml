	<select id="selectPaginationByPageNum"
		resultMap="ZyCorporationResult" parameterClass="map">
		select RowID, ID, Password, name, areaID, tradeID, activation,
		IS50, IS100, orderno, IsNew, Area, Address, TaxNumber,
		WorkerName, Tel, Mobile, Email, Postcode, Faxes, Infor, Remark,
		WebUrl, ByOrder, Bak1, Bak2, Bak3, Bak4, Bak5, Bak6 from
		zy_corporation
		<dynamic prepend="where">
			<isNotEmpty prepend="and" property="id">ID=#id#</isNotEmpty>
			<isNotEmpty prepend="and" property="name">
				name like '%$name$%'
			</isNotEmpty>
		</dynamic>
		order by RowID desc limit #start#,#size#
	</select>


